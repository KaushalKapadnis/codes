#include<stdio.h>

///////////////////////////////////////////////////////////////////////////////////
//
//	Name : Display
//	Input : Integer
//	Description : Display Integer from one to the Number Entered as Input
//	Author : Kuashal Sachin Kapadnis 
//	Date : 
//
///////////////////////////////////////////////////////////////////////////////////

void Display(int iNo)
{
	int iCnt = 0;
	if(iNo < 0)
	{
		iNo = -iNo;
	}
	for(iCnt = 1; iCnt <= iNo; iCnt++)
	{
		printf("%d\t",iCnt);
	}
}

///////////////////////////////////////////////////////////////////////////////////
//	Entry point function
///////////////////////////////////////////////////////////////////////////////////

int main()
{
	int iValue;
	
	printf("Enter the number :");
	scanf("%d",&iValue);
	
	Display(iValue);
	
	return 0;
}
